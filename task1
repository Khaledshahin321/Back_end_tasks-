#include<iostream>
using namespace std;
int main()
{
    int x,y,n ,add_two_numbers , multiply_two_numbers
,subtract_two_numbers , Divide_two_numbers;
  cout<<"\n"<<"Please Enter Two Numbre : "<<"\n";
  cin>>x>>y;
  cout<<"\n"<<"please Enter Your Choice : \n 1)add two numbers \n 2) multiply two numbers  \n 3)subtract two numbers \n   4) Divide two numbers \n ";
cin>>n;
switch(n)
{
case 1:
 add_two_numbers= x+y;
 cout <<"\n"<< add_two_numbers<<"\n";
 break ;
case 2:
 multiply_two_numbers=x*y;
 cout <<"\n"<< multiply_two_numbers <<"\n";
 break ;
case 3:
 subtract_two_numbers=x-y;
 cout <<"\n"<< subtract_two_numbers <<"\n";
 break;
case 4:
 Divide_two_numbers=x%y;
 cout <<"\n"<< Divide_two_numbers <<"\n";
 break;
 default:
 cout<<"wrong choice Please Enter Valid Choice......";
 break;
}
return 0;
}
